# TMDB API 키 (https://www.themoviedb.org/settings/api 에서 발급)
VITE_TMDB_API_KEY=your_tmdb_api_key_here
TMDB_API_KEY=your_tmdb_api_key_here
TMDB_ACCESS_TOKEN=your_tmdb_access_token_here

# YouTube API 키 (https://console.cloud.google.com/ 에서 발급)
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here

# Django 시크릿 키 (python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())' 로 생성)
SECRET_KEY=your_django_secret_key_here

# Debug 모드 (개발: True, 프로덕션: False)
DEBUG=True

# 허용된 호스트 (쉼표로 구분)
ALLOWED_HOSTS=localhost,127.0.0.1

# CORS 허용 오리진 (쉼표로 구분)
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
